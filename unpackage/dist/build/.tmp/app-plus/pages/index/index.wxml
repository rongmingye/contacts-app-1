<view class="home_content" bind:__l="__l"><view class="header"><button class="get_btn" size="mini" type="primary" data-event-opts="{{[['tap',[['getContacts',['$event']]]]]}}" bindtap="__e">获取手机联系人</button><button class="upload_btn" size="mini" type="primary" data-event-opts="{{[['tap',[['uploadContacts',['$event']]]]]}}" bindtap="__e">同步到后台</button></view><view class="contact_list"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="contact_item"><view class="name">{{item.displayName}}</view><block wx:for="{{item.phoneNumbers}}" wx:for-item="subitem" wx:for-index="idx" wx:key="idx"><view class="phone">{{subitem.value}}</view></block></view></block></view></view>