<view class="register_content" bind:__l="__l"><view class="uni-common-mt"><view class="uni-form-item uni-column"><view class="title">账号(手机号):</view><input class="uni-input" id="account" placeholder="请输入账号" data-event-opts="{{[['input',[['__set_model',['$0','account','$event',[]],['user']]]]]}}" value="{{user.account}}" bindinput="__e"/></view><view class="uni-form-item uni-column"><view class="title">昵称:</view><input class="uni-input" id="nickname" placeholder="请输入昵称" data-event-opts="{{[['input',[['__set_model',['$0','nickname','$event',[]],['user']]]]]}}" value="{{user.nickname}}" bindinput="__e"/></view><view class="uni-form-item uni-column"><view class="title">密码:</view><input class="uni-input" type="password" id="password" placeholder="请输入密码" data-event-opts="{{[['input',[['__set_model',['$0','password','$event',[]],['user']]]]]}}" value="{{user.password}}" bindinput="__e"/></view><view class="uni-form-item uni-column"><view class="title">再次输入密码:</view><input class="uni-input" type="password" id="againPassword" placeholder="请再次输入密码" data-event-opts="{{[['input',[['__set_model',['$0','againPassword','$event',[]],['user']]]]]}}" value="{{user.againPassword}}" bindinput="__e"/></view><view class="uni-form-item footer"><button type="primary" data-event-opts="{{[['tap',[['submitRegister']]]]}}" bindtap="__e">注册</button></view></view></view>